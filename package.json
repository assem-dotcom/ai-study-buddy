{
  "name": "study-helper",
  "version": "1.0.0",
  "description": "AI-powered study helper that generates quizzes, summaries, podcasts, and acts as a tutor",
  "type": "module",
  "main": "backend/index.js",
  "scripts": {
    "start": "node --experimental-modules backend/index.js",
    "dev": "nodemon --experimental-modules backend/index.js",
    "client": "cd frontend && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.2",
    "@types/axios": "^0.9.36",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "groq": "^3.86.1",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.96.0",
    "pdf-parse": "1.1.1"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22"
  }
}
